class Termostato{
  
  constructor(){
    
  }
  
  dimeTemperatura(temperatura){
    console.log("Temperatura:",temperatura);
  }
}



class Aire{
  constructor(){
    this.termostato=new Termostato();
  }
  
  encender(){
    console.log("On");
  }
  
  apagar(){
    console.log("Off");
  }
  
}



class Frío extends Aire{
    constructor(){
    super()
    this.modo="-";
  }
  
  encender(){
   console.log("On");
   this.temperatura=20 ;
   this.modo="frío";
   console.log("Modo:",this.modo);
   this.termostato.dimeTemperatura(this.temperatura);
  }
    
}


class Calefacción extends Aire{
     constructor(){
    super()
    this.modo="-";
  }
  
  encender(){
   console.log("On");
   this.temperatura= 27 
   this.modo="calor"
   console.log("Modo:",this.modo);
   this.termostato.dimeTemperatura(this.temperatura); 
  }
    
}


let airefrío
let airecalor
 function setup(){
  airefrío=new Frío();
  airecalor=new Calefacción()
   
   
  airefrío.encender();
 
}

class Calefacción extends Aire{
     constructor(){
    super()
    this.modo="-";
  }
  
  encender(){
   console.log("On");
   this.temperatura= 27 
   this.modo="calor"
   console.log("Modo:",this.modo);
   this.termostato.dimeTemperatura(this.temperatura); 
  }
    
}


let airefrío
let airecalor
 function setup(){
  airefrío=new Frío();
  airecalor=new Calefacción()
   
   
  airecalor.encender();
